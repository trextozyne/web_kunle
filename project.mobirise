        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 10px)",
            "padding-bottom": "(@paddingBottom * 10px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "> .mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card": {
              "margin-bottom": "30px",
              "& when not (@transparentCard)": {
                ".inner-item": {
                  "background-color": "@cardBG",
                  "position": "relative",
                  "height": "100%",
                  "box-shadow": "0px 0px 35px 0px rgba(0, 0, 0, 0.05)"
                }
              },
              "& when (@showImg)": {
                ".image": {
                  "height": "(@imgHeight * 1px)",
                  "min-height": "200px",
                  "width": "100%",
                  "position": "relative",
                  "overflow": "hidden",
                  "amp-img, img": {
                    "& when (@imgEffect)": {
                      "filter": "grayscale(100%)"
                    },
                    "& when not (@transparentCard)": {
                      "border-bottom-left-radius": "0",
                      "border-bottom-right-radius": "0"
                    }
                  }
                },
                "& when (@imgEffect)": {
                  ".mbr-overlay": {
                    "opacity": "@imgOverlayOpacity",
                    "z-index": "1",
                    "pointer-events": "none",
                    "& when not (@transparentCard)": {
                      "border-bottom-left-radius": "0",
                      "border-bottom-right-radius": "0"
                    }
                  },
                  ".item1 .mbr-overlay": {
                    "background-color": "@imgOverlayColor1"
                  },
                  ".item2 .mbr-overlay": {
                    "background-color": "@imgOverlayColor2"
                  },
                  ".item3 .mbr-overlay": {
                    "background-color": "@imgOverlayColor3"
                  },
                  ".item4 .mbr-overlay": {
                    "background-color": "@imgOverlayColor4"
                  }
                }
              }
            },
            "& when (@showImgIcon) and not (@transparentCard)": {
              ".icon__image": {
                "bottom": "-1px",
                "width": "(@IconSize * 2px)",
                "height": "(@IconSize * 1.6px)",
                "z-index": "9",
                "position": "absolute",
                "padding": "10px 0 0",
                "background-color": "@cardBG",
                "border-top-right-radius": "7px",
                "border-top-left-radius": "7px",
                "left": "~'calc(50% - @{IconSize}px)'",
                ".mbr-iconfont": {
                  "font-size": "(@IconSize * 1px)",
                  "width": "(@IconSize * 1px)"
                }
              },
              ".itemImg1": {
                "color": "@imgOverlayColor1"
              },
              ".itemImg2": {
                "color": "@imgOverlayColor2"
              },
              ".itemImg3": {
                "color": "@imgOverlayColor3"
              },
              ".itemImg4": {
                "color": "@imgOverlayColor4"
              }
            },
            ".card-text": {
              "padding": "0 35px 38px"
            },
            "& when (@showCardsTitle)": {
              ".card__title": {
                "margin-top": "26px"
              }
            },
            "& when (@showCardsText)": {
              ".divider": {
                "width": "30px",
                "background-color": "contrast(@cardBG)",
                "opacity": ".3"
              }
            },
            "& when (@showCardsButtons)": {
              ".btn, .mbr-section-btn": {
                "margin-left": "0",
                "margin-right": "0"
              },
              ".btn": {
                "margin-top": "30px",
                "width": "100%",
                ".btn__hover": {
                  "transition": "transform 0.4s ease"
                },
                ".mbr-iconfont": {
                  "position": "relative",
                  "opacity": "0",
                  "margin-top": "1px",
                  "transition": "opacity 0.15s ease, .15s width 0s",
                  "width": "0"
                },
                "&:hover": {
                  ".mbr-iconfont": {
                    "opacity": "1",
                    "width": "15px"
                  },
                  ".btn__hover": {
                    "transform": "translateX(-10px)"
                  }
                }
              }
            }
          },
          "_name": "features2",
          "_customHTML": "<section class=\"features2\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n            <header>Cards</header>\n        <select title=\"CardsAmount\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected=\"\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"checkbox\" title=\"Transparent Card\" name=\"transparentCard\">\n        <input type=\"color\" title=\"Card Background\" name=\"cardBG\" value=\"#ffffff\" condition=\"!transparentCard\">\n        <header>Cards Text</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n        <header>Cards Image</header>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"200\" max=\"600\" step=\"10\" value=\"200\" condition=\"showImg\">\n        <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\" condition=\"showImg\">\n        <input type=\"color\" title=\"Color 1\" name=\"imgOverlayColor1\" value=\"#72a0fb\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=1 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 2\" name=\"imgOverlayColor2\" value=\"#72a0fb\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=2 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 3\" name=\"imgOverlayColor3\" value=\"#72a0fb\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=3 &amp;&amp; showImg\">\n        <input type=\"color\" title=\"Color 4\" name=\"imgOverlayColor4\" value=\"#21ac13\" condition=\"((showImgIcon &amp;&amp; !transparentCard) || imgEffect) &amp;&amp; cardsAmount >=4 &amp;&amp; showImg\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.3\" condition=\"showImg &amp;&amp; imgEffect\">\n        <input type=\"checkbox\" title=\"Show Image Icon\" name=\"showImgIcon\" condition=\"showImg &amp;&amp; !transparentCard\">\n        <input type=\"range\" title=\"Icon Size\" name=\"IconSize\" min=\"20\" max=\"40\" step=\"5\" value=\"30\" condition=\"showImg &amp;&amp; showImgIcon &amp;&amp; !transparentCard\">\n        <!--  -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/ect24.jpg\">\n        </fieldset>\n        <!--  -->\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#3b7eff\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"\" mbr-class=\"{'container':!fullWidth,\n                    